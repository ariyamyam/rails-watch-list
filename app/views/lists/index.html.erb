<h1>My Best Movie Watch List</h1>
<h4>My Lists</h4>

<%# show existing lists %>
<ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ul>

<%# link to create a new list with link_to ANCHOR TEXT, ANCHOR PATH%>
<div>
<%= link_to "Create a new movie list", new_list_path, class: "btn btn-primary" %>
</div>
